name: "My first workflow"

on: push

jobs:
  first_job:
    runs-on: ubuntu-latest
    steps:
    - name: Checkout Repo
      uses: actions/checkout@v4
      
    - name: Welcome Message
      run: echo "My first step github action"
    
    - name: list of file
      run: ls
    
    - name: Read files
      run: README.md